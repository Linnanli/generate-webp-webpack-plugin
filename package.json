{
  "name": "generate-webp-webpack-plugin",
  "version": "0.1.2",
  "description": "Webpack plugin for converting images to webp format",
  "main": "src/index.js",
  "keywords": [
    "webpack",
    "webp",
    "plugin"
  ],
  "author": "nally.lin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Linnanli/generate-webp-webpack-plugin.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "commit": "git-cz",
    "release": "standard-version",
    "major": "standard-version -r major",
    "minor": "standard-version -r minor",
    "patch": "standard-version -r patch",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run test"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "imagemin-webp-copy": "^5.1.1",
    "lodash.clonedeep": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^24.0.25",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.9",
    "cz-conventional-changelog": "^3.0.2",
    "file-loader": "^5.0.2",
    "file-type": "^16.5.4",
    "fs-extra": "^8.1.0",
    "husky": "^4.0.6",
    "jest": "^24.9.0",
    "standard-version": "^7.0.1",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5"
  }
}
